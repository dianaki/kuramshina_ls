.app-container {
  min-height: 100vh;
  display: flex;
  flex-direction: column;
}

.page-content {
  padding: 50px 0 15px;
  flex: 1;
  background: url("../images/bg/admin.jpg") center center / cover no-repeat;
}

.header {
  display: flex;
  margin-bottom: 60px;
}

.title {
  font-size: 21px;
  font-weight: bold;
  color: #414C63;
  margin-right: 54px;
}

.skills {
  display: flex;
  flex-wrap: wrap;
}

.item {
  width: calc(100% / 2 - 24px);
  margin-right: 24px;
}